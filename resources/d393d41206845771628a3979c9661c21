<script type="text/javascript">
	function searchFormSubmit2(form) {
		qfield = jQuery("#gsaphilly").val();
		if(qfield != null) {
			if(qfield == "") {
				jQuery('#search').hide();
				jQuery('#overlay_gradient').css({display:'none'});
				jQuery('#overlay_gradient').removeClass('overlay_gradientActive');
			return false;
			}
			return true;
		}
	}
</script>
