<script type="text/javascript">
   jQuery(document).click(function(e) {
	var focusSearch = jQuery("#gsaphilly").is(":focus");
	if (!focusSearch) {
		jQuery('#search').hide();
		jQuery('#overlay_gradient').css({display:'none'});
		jQuery('#overlay_gradient').removeClass('overlay_gradientActive');
	}
   });
  jQuery('#searchBtn a').click(function(e){
	e.preventDefault();
	jQuery('#overlay_gradient').css({display:'block'});
	jQuery('#overlay_gradient').addClass('overlay_gradientActive');
	jQuery("#search").show();
	jQuery("#gsaphilly").focus();
	 e.stopPropagation();
  });
