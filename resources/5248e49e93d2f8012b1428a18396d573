<script type="text/javascript">
  

  load_trinity(function(){
    //load_placement sets the keys returned from the load_trinity method and ultimately sends them to OpenX
    //The argument of the load_placement method is the openx-adunitid used in the association object
    load_placement("538225446");
});
</script>

