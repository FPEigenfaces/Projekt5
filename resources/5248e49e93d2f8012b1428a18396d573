 <script>

window.load_trinity = function(callback, force) {
    //Load our script to provide key - values which are set to the global variable sbi_trinity
    //Add more associations to thisobject as necessary
    var associations = {};
    //300x250
    associations['538225446'] = '93d94cc1180d7e9fd7a2';
     
    if(callback == null) callback = function(){};
    if(force == null) force = false;
     
    //If a call has already been made,
    if('sbi_trinity' in window && !force) {
        callback();
        callback = function(){};
    }
     
    var sbi_script = document.createElement('script');
    sbi_script.async = true;
    sbi_script.type = 'text/javascript';
    sbi_script.src = 'http://apex.go.sonobi.com/trinity.js?key_maker=' + JSON.stringify(associations) + '&s=' + Math.floor(Math.random()* 1000);   
     
    if(sbi_script.readyState) {
        sbi_script.onreadystatechange = function() {
            if(sbi_script.readyState == "loaded" || sbi_script.readyState == "complete") {
                sbi_script.onreadystatechange = null;
                callback();
                callback = function(){};
            }
        };
    } else {
        sbi_script.onload = function () {
            callback();
            callback = function(){};
        };
    }
 
    var node = document.getElementsByTagName('script')[0];
    node.parentNode.insertBefore(sbi_script, node);
     
    //The following code ensures that the callback method is run after no more than 800 milliseconds
    setTimeout(function(){
        callback();
        callback = function(){};
    }, 800);
};

var load_placement = function(id) {
    var OX_4d6552943f5a4 = OX();
  OX_4d6552943f5a4.addAdUnit(id);
  console.debug(sbi_trinity);
  OX_4d6552943f5a4.setAdUnitSlotId(id,"538225446_HP_RAIL_300x250");
  OX_4d6552943f5a4.addVariable("sbi_aid", sbi_trinity[id].sbi_aid);
  console.debug(sbi_trinity[id].sbi_aid);
  OX_4d6552943f5a4.addVariable("sbi_apoc", sbi_trinity[id].sbi_apoc);
  console.debug(sbi_trinity[id].sbi_apoc);
  OX_4d6552943f5a4.addVariable("sbi_size", sbi_trinity[id].sbi_size);
  console.debug(sbi_trinity[id].sbi_size);
  OX_4d6552943f5a4.addVariable("sbi_dc", sbi_trinity[id].sbi_dc);
  console.debug(sbi_trinity[id].sbi_dc);
  OX_4d6552943f5a4.load();
};



// </script>

